#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i,max;
    scanf("%d",&n);
    int a[n];
    a[0]=max;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        if(max<a[i])
            max=a[i];
    }
    printf("%d",max);

    return 0;
}
