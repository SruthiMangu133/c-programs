#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,r,sum;
    scanf("%d",&n);
    sum=0;
    while(n!=0){
        r=n%10;
          
        if(r%2!=0){
            sum=sum+r;
        }
        n=n/10;
      
    }
    printf("%d",sum);
    return 0;
}
