#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,n;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++){
        a[i]=a[i-1]+a[i];
    }
    for(i=0;i<n;i++){
    printf("%d ",a[i]);
    }
    

    
    return 0;
}
